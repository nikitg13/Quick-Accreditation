                                    <tbody>
                                        <tr>
                                            <td>Total number of regular faculty </td>
                                            <td>
                                                <%=FacultyInfo[0].regularCAYm2%>
                                            </td>
                                        </tr>

                                        <tr>
                                            <td>Total number of contractual faculty </td>
                                            <td>
                                                <%=FacultyInfo[0].contractualCAYm2%>
                                            </td>
                                        </tr>

                                        <tr>
                                            <td>Total number of Ph.D faculty </td>
